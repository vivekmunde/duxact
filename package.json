{
  "name": "duxact",
  "version": "2.4.2",
  "description": "Application state management for React",
  "main": "./index.js",
  "module": "./lib/es/index.js",
  "types": "./lib/es/index.d.ts",
  "scripts": {
    "build-development": "webpack --config webpack.development.config.js",
    "build-production": "webpack --config webpack.production.config.js",
    "build": "npm run clean && npm run compile-ts",
    "clean": "rimraf ./lib",
    "compile-ts": "tsc -p tsconfig-esm.json && tsc -p tsconfig-cjs.json",
    "test": "npm run compile-ts && jest",
    "prepublish": "npm run clean && npm run build"
  },
  "author": "Vivek Munde",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivekmunde/duxact.git"
  },
  "files": [
    "lib",
    "src",
    "index.js"
  ],
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "react state management"
  ],
  "homepage": "https://github.com/vivekmunde/duxact",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
  }
}
